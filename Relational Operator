package JavaPackage;

public class RelationalOperator1 {

	public static void main(String[] args) {
		int a=100;
		
		int b=100;
		
		if (a!=b) {
			System.out.println("a is not equal to b");
		}
		else {
			System.out.println("a is equal to b");// here the answer is a is equal to b
			
	         a  =100;
			
			 b=10;
			if (a!=b) {
				System.out.println("a is not equal to b");
			}
			else {
				System.out.println("a is equal to b");// here the answer is a is not  equal to b
