package JavaPackage;

public class MethodOverloading1 {
	
	public void sum(int a,int b) 
	
	
	{
		
		System.out.println(a+b);
	}
		
		public void sum(int a,int b,int c) {
			
			System.out.println(a+b+c);
		}
		
 public static void main (String[] args) {

	
	 
	 int a=10;
		int b =20;
		
	 System.out.println(a+b);
	
	}
	}
//output is 30.
