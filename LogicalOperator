package JavaPackage;

public class LogicalOperator {

	public static void main(String[] args) {
		int a=1;
        int b=5;
        int c=15;
        System.out.println(a<c && b>a);//It doesnt check the second condition if 1st condition  is false
        System.out.println(a>c && b>a);//It doesnt check the second condition if 1st condition  is false
	}

}
